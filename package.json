{
  "name": "luckystarry-collections",
  "version": "0.1.0-alpha.2",
  "description": "模仿 .NET 的集合类",
  "keywords": [
    "collections"
  ],
  "scripts": {
    "clean": "del-cli dist -f",
    "lint": "tslint -p . -c tslint.json 'src/**/*.ts'",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "postbuild": "npm run bundle",
    "bundle": "browserify dist/index.js > dist/index.bundle.js",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "extension": [".ts"],
    "reporter": ["html", "lcov", "text", "text-summary"],
    "include": ["src"],
    "all": true
  },
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.2",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "should": "^13.2.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.3",
    "uglifyify": "^5.0.1",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh:git@github.com:LuckyStarry/luckystarry-collections.git"
  },
  "bugs": {
    "email": "starry@vip.qq.com",
    "url": "https://github.com/LuckyStarry/luckystarry-collections/issues"
  },
  "author": {
    "name": "SUN BO",
    "email": "starry@vip.qq.com",
    "url": "https://www.luckystarry.com"
  }
}